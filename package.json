{
    "name": "eef-budget-calculator",
    "version": "1.0.0",
    "description": "Web app to manage family budget",
    "main": "index.ts",
    "scripts": {
        "api-compile": "dotnet msbuild ./src_webapi/BudgetCalculationAPI.sln /t:Restore;Rebuild /p:Configuration=Release /p:Platform=\"any cpu\"",
        "api-build": "dotnet publish ./src_webapi/BudgetCalculationAPI/BudgetCalculationAPI.csproj --output=../../.dist_api --configuration=Release",
        "ui-build": "webpack",
        "web-run-server:dev": "webpack-dev-server",
        "start": "npm run web-run-server:dev",
        "test": "echo \"Error: no test specified\" && exit 1",
        "tslint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/EugeneElkin/eef-budget-calculator.git"
    },
    "author": "Evgenii Elkin",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/EugeneElkin/eef-budget-calculator/issues"
    },
    "homepage": "https://github.com/EugeneElkin/eef-budget-calculator#readme",
    "devDependencies": {
        "@types/react": "^16.1.0",
        "@types/react-dom": "^16.0.4",
        "@types/react-redux": "^6.0.9",
        "app-root-path": "^2.1.0",
        "awesome-typescript-loader": "^5.2.1",
        "copy-webpack-plugin": "^4.6.0",
        "css-loader": "^1.0.1",
        "html-webpack-plugin": "^3.2.0",
        "source-map-loader": "^0.2.4",
        "style-loader": "^0.23.1",
        "tslint": "^5.11.0",
        "typescript": "^3.1.6",
        "webpack": "^4.25.1",
        "webpack-cli": "^3.1.2",
        "webpack-dev-server": "^3.2.0"
    },
    "dependencies": {
        "@types/uuid": "^3.4.4",
        "axios": "^0.18.0",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-redux": "^5.1.1",
        "redux": "^4.0.1",
        "uuid": "^3.3.2"
    }
